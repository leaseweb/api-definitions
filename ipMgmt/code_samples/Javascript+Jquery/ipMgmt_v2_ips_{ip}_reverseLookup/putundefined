const settings = {
  "async": true,
  "crossDomain": true,
  "url": "https://api.leaseweb.com/ipMgmt/v2/ips/192.0.2.1/reverseLookup",
  "method": "PUT",
  "headers": {
    "X-LSW-Auth": "REPLACE_KEY_VALUE",
    "content-type": "application/json"
  },
  "processData": false,
  "data": "{\"reverseLookups\":[{\"ip\":\"string\",\"reverseLookup\":\"string\"}]}"
};

$.ajax(settings).done(function (response) {
  console.log(response);
});