import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

headers = { 'X-LSW-Auth': "REPLACE_KEY_VALUE" }

conn.request("GET", "/ipMgmt/v2/ips/192.0.2.1/nullRouted?ip=SOME_STRING_VALUE&limit=20&offset=10", headers=headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))