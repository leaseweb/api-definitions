import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

headers = { 'X-LSW-Auth': "REPLACE_KEY_VALUE" }

conn.request("DELETE", "/hosting/v2/domains/example.com/mailboxes/%7BemailAddress%7D/autoResponder", headers=headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))