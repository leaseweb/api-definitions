import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

payload = "{\"dotNetVersion\":\"string\"}"

headers = {
    'X-LSW-Auth': "REPLACE_KEY_VALUE",
    'content-type': "application/json"
    }

conn.request("POST", "/hosting/v2/webhosting/example.com/%7BequipmentId%7D/dotNet", payload, headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))