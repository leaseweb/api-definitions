import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

headers = { 'X-LSW-Auth': "REPLACE_KEY_VALUE" }

conn.request("GET", "/abuse/v1/reports?limit=20&offset=10&status=OPEN%2CWAITING%2CCLOSED&ticketId=SOME_STRING_VALUE&ip=SOME_STRING_VALUE", headers=headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))