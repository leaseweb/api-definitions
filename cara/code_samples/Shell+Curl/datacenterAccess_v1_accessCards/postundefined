  --url https://api.leaseweb.com/datacenterAccess/v1/accessCards \
  --header 'X-LSW-Auth: REPLACE_KEY_VALUE' \
  --header 'content-type: application/json' \
  --data '{"email":"string","hasUser":true,"firstName":"string","lastName":"string","phoneNumber":"string","multipleAccess":true,"allowedItems":["string"],"allItemsAllowed":true,"hasPermanentBadge":true}'