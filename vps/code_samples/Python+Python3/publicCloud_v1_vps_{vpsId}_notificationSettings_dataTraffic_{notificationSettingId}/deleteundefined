import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

headers = { 'X-LSW-Auth': "REPLACE_KEY_VALUE" }

conn.request("DELETE", "/publicCloud/v1/vps/999999999/notificationSettings/dataTraffic/3a042956-0689-45dc-8322-8b8325464182", headers=headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))