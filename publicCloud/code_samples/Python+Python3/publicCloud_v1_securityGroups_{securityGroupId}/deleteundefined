import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

headers = { 'X-LSW-Auth': "REPLACE_KEY_VALUE" }

conn.request("DELETE", "/publicCloud/v1/securityGroups/166cc685-8273-11f0-b4f8-2687367a8a5e", headers=headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))