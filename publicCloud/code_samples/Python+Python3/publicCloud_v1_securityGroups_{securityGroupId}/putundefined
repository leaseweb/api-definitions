import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

payload = "{\"name\":\"string\"}"

headers = {
    'X-LSW-Auth': "REPLACE_KEY_VALUE",
    'content-type': "application/json"
    }

conn.request("PUT", "/publicCloud/v1/securityGroups/166cc685-8273-11f0-b4f8-2687367a8a5e", payload, headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))