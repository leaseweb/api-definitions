import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

payload = "{\"rules\":[\"497f6eca-6276-4993-bfeb-53cbbbba6f08\"]}"

headers = {
    'X-LSW-Auth': "REPLACE_KEY_VALUE",
    'content-type': "application/json"
    }

conn.request("POST", "/publicCloud/v1/securityGroups/166cc685-8273-11f0-b4f8-2687367a8a5e/revokeFirewallRules", payload, headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))