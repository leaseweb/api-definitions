import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

payload = "{\"securityGroupIds\":[\"497f6eca-6276-4993-bfeb-53cbbbba6f08\"]}"

headers = {
    'X-LSW-Auth': "REPLACE_KEY_VALUE",
    'content-type': "application/json"
    }

conn.request("POST", "/publicCloud/v1/instances/ace712e9-a166-47f1-9065-4af0f7e7fce1/attachSecurityGroups", payload, headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))