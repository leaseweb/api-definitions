import http.client

conn = http.client.HTTPSConnection("api.leaseweb.com")

headers = { 'X-LSW-Auth': "REPLACE_KEY_VALUE" }

conn.request("DELETE", "/publicCloud/v1/instances/ace712e9-a166-47f1-9065-4af0f7e7fce1/snapshots/54eee4c2-aef2-4cda-9fd5-74bfbf438abf", headers=headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))