const settings = {
  "async": true,
  "crossDomain": true,
  "url": "https://api.leaseweb.com/cdn/v2/certificates",
  "method": "POST",
  "headers": {
    "X-LSW-Auth": "REPLACE_KEY_VALUE",
    "content-type": "application/json"
  },
  "processData": false,
  "data": "{\"description\":\"string\",\"certificate\":\"string\",\"privateKey\":\"string\",\"caBundle\":\"string\",\"validation\":true}"
};

$.ajax(settings).done(function (response) {
  console.log(response);
});