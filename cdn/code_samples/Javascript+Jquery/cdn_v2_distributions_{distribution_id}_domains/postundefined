const settings = {
  "async": true,
  "crossDomain": true,
  "url": "https://api.leaseweb.com/cdn/v2/distributions/%7Bdistribution_id%7D/domains",
  "method": "POST",
  "headers": {
    "X-LSW-Auth": "REPLACE_KEY_VALUE",
    "content-type": "application/json"
  },
  "processData": false,
  "data": "{\"domain\":\"string\"}"
};

$.ajax(settings).done(function (response) {
  console.log(response);
});